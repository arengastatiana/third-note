public class Main
{
    public static void main(String[] args) {
        
        int edad = 19;
        double altura = 1.64; 
        char inicialNombre = 'T';
        boolean mayorEdad = edad >=19;
        
        System.out.println("mi edad es:" + edad);
        System.out.println("Mi estatura es:"  + altura);
        System.out.println("La inicial de mi nombre es: "+ inicialNombre);
        System.out.println("Es mayor de edad: " + mayorEdad);
        
        
        
        
    }
}
